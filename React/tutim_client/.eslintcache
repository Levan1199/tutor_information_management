\r\n        this.toggleNav = this.toggleNav.bind(this);\r\n        this.toggleModal = this.toggleModal.bind(this);\r\n        this.handleLogin = this.handleLogin.bind(this);\r\n        this.handleLogout = this.handleLogout.bind(this);\r\n    }\r\n\r\n    toggleNav(){\r\n        this.setState({\r\n            isNavOpen: !this.state.isNavOpen\r\n        });\r\n    }\r\n\r\n    toggleModal(){\r\n        this.setState({\r\n            isModalOpen: !this.state.isModalOpen\r\n        });\r\n    }\r\n    \r\n    toggleDropdown(){\r\n        this.setState({\r\n            isDropdownOpen: !this.state.isDropdownOpen\r\n        });\r\n    }\r\n\r\n    handleLogin(event){\r\n        this.toggleModal();\r\n        this.props.loginUser({username: this.username.value, password: this.password.value});\r\n        event.preventDefault();\r\n    }\r\n\r\n    handleLogout(){\r\n        this.props.logoutUser();\r\n    }\r\n\r\n    render(){\r\n        return( \r\n            <>\r\n                <Navbar light expand=\"md\">\r\n                    <div className=\"container\">\r\n                        <NavbarToggler onClick={this.toggleNav}/>\r\n                        <NavbarBrand className=\"mr-auto\" href=\"/\">\r\n                            {/* <img src=\"assets/images/logo.png\" height=\"30\" width=\"41\" alt=\"Ristorante Con Fusion\" /> */}\r\n                            <img src=\"/logo512.png\" height=\"30\" width=\"41\"\r\n                                alt=\"Gia Su Viet\" />\r\n                        </NavbarBrand>\r\n                        <Collapse isOpen={this.state.isNavOpen} navbar>\r\n                            <Nav navbar>\r\n                                <NavItem>\r\n                                    <NavLink className=\"nav-link\" to=\"/home\">\r\n                                        <span className=\"fa fa-home fa-lg\"></span> Trang chủ\r\n                                    </NavLink>\r\n                                </NavItem>\r\n                                <UncontrolledDropdown setActiveFromChild>\r\n                                    <DropdownToggle className=\"nav-link\" color=\"\">\r\n                                        <span className=\"fa fa-address-book fa-lg\"></span> Phụ huynh\r\n                                    </DropdownToggle>\r\n                                    <DropdownMenu>\r\n                                        <DropdownItem  href=\"/teacherList\" >Gia sư hiện có</DropdownItem>\r\n                                        <DropdownItem  href=\"/findTeacher\" >Đăng ký tìm gia sư</DropdownItem>\r\n                                    </DropdownMenu>                                    \r\n                                </UncontrolledDropdown>\r\n\r\n                                <UncontrolledDropdown setActiveFromChild>\r\n                                    <DropdownToggle className=\"nav-link\" color=\"\">\r\n                                        <span class=\"fa fa-users fa-lg\"></span> Gia sư\r\n                                    </DropdownToggle>\r\n                                    <DropdownMenu>\r\n                                        <DropdownItem  href=\"/studentList\" >Lớp học hiện có</DropdownItem>\r\n                                        <DropdownItem  href=\"/findClass\" >Đăng ký tìm lớp</DropdownItem>\r\n                                    </DropdownMenu>                                    \r\n                                </UncontrolledDropdown>\r\n                            </Nav>\r\n                            <Nav className=\"ml-auto\" navbar>\r\n                                <NavItem>\r\n                                    { !this.props.auth.isAuthenticated ?\r\n                                        <Button onClick={this.toggleModal}>\r\n                                            <span className=\"fa fa-sign-in fa-lg\"></span> Đăng nhập\r\n                                            {this.props.auth.isFetching ?\r\n                                                <span className=\"fa fa-spinner fa-pulse fa-fw\"></span>\r\n                                                : null\r\n                                            }\r\n                                        </Button>\r\n                                        :\r\n                                        <div>\r\n                                        <Link to=\"/teacherInfo\"><div className=\"navbar-text mr-3\">{this.props.auth.user.username}</div></Link>\r\n                                        <Button onClick={this.handleLogout}>\r\n                                            <span className=\"fa fa-sign-out fa-lg\"></span> Đăng xuất\r\n                                            {this.props.auth.isFetching ?\r\n                                                <span className=\"fa fa-spinner fa-pulse fa-fw\"></span>\r\n                                                : null\r\n                                            }\r\n                                        </Button>\r\n                                        </div>\r\n                                    }\r\n\r\n                                </NavItem>\r\n                            </Nav>\r\n                            \r\n                        </Collapse>\r\n                    </div>\r\n                </Navbar>\r\n        \r\n                <Modal isOpen={this.state.isModalOpen} toggle={this.toggleModal}>\r\n                    <ModalHeader toggle={this.toggleModal}>Đăng nhập</ModalHeader>\r\n                    <ModalBody>\r\n                        <Form onSubmit={this.handleLogin}>\r\n                            <FormGroup>\r\n                                <Label htmlFor=\"username\">Tên tài khoản</Label>\r\n                                <Input type=\"text\" id=\"username\" name=\"username\"\r\n                                innerRef={(input)=>this.username=input}/>\r\n                            </FormGroup>\r\n                            <FormGroup>\r\n             