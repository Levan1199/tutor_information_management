ainer\">\r\n                <div className=\"row row-content\">\r\n                    <div className=\"col-md-12 col-lg-3\">\r\n                        <img class=\"ava\" src=\"assets/images/download.png\" alt=\"Teacher's photo\" />\r\n                    </div>\r\n                    <div className=\"col-md-12 col-lg-7\">\r\n                        <h2>{props.profile.name}</h2>\r\n                        <h4>{props.profile.email}</h4>\r\n                        <hr/>\r\n                        <h6>\r\n                            <strong>Mô tả: </strong> {props.profile.description}\r\n                        </h6>                 \r\n                    </div>\r\n                    <div className=\"col-md-12 col-lg-2\">\r\n                        <CommentForm/>\r\n                    </div>\r\n                </div>\r\n\r\n                <RenderWorkSpace profile={props.profile}/>\r\n\r\n                <div className=\"row justify-content-center\">\r\n                    <h4 className=\"font-weight-bold\">\r\n                        Chứng chỉ\r\n                    </h4>\r\n                </div>\r\n                <div className=\"row align-items-start\">\r\n                    <div className=\"col-12 col-md m-1\">\r\n                        <RenderCard/>\r\n                    </div>\r\n                    <div className=\"col-12 col-md m-1\">\r\n                        <RenderCard/>\r\n                    </div>\r\n                    <div className=\"col-12 col-md m-1\">\r\n                        <RenderCard/>\r\n                    </div>            \r\n                </div>\r\n                <hr/>\r\n                <div className=\"col-12\">\r\n                    <h3>Đánh giá</h3>\r\n                </div>\r\n                <RenderUserComment/>\r\n                <RenderUserComment/>\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nexport default TeacherInfo;","C:\\VanHung\\WebThesis\\React\\tutim_client\\src\\components\\list\\studentRegs.js"